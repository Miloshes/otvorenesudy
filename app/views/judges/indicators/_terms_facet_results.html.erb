<% if other.count == 1 && other.first.missing? %>
  <%= render 'judges/indicators/facet_results', facet: facet, visible: visible + other, other: [], link: method(:link_to_terms_facet), options: options  %>
<% else %>
  <%= render 'judges/indicators/facet_results', facet: facet, visible: visible, other: other, link: method(:link_to_terms_facet) , options: options %>
<% end %>
